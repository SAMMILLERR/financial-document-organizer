# Test Environment Variables
# Copy this to .env.test and fill in test values

# Application
PORT=3001

# Google OAuth Credentials (use test/mock credentials)
GOOGLE_CLIENT_ID=test-client-id
GOOGLE_CLIENT_SECRET=test-client-secret
GOOGLE_REDIRECT_URI=http://localhost:3001/auth/callback

# Google Resources (use test sheet/folder)
GOOGLE_SHEET_ID=test-sheet-id
GOOGLE_DRIVE_FOLDER_ID=test-folder-id

# Local Storage
TOKEN_DB_PATH=./test-tokens.json
